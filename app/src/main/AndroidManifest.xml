<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.svs.markupui">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".app.MarkupUi"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon_01"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_01_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity android:name="com.svs.markupui.view.activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.svs.markupui.view.activities.MainHome"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>